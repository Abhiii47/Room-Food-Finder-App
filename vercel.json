{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/room-food-finder-frontend/index.html"
    }
  ],
  "builds": [
    {
      "src": "room-food-finder-frontend/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ]
}